@{
    ViewData["Title"] = "Privacy Policy";
    @model EmprendedorModel
}
<h1>@ViewData["Title"]</h1>

<p>Use this page to detail your site's privacy policy.</p>

<div class="row">
    <div class="display-4">
        <label for="nombre">El nombre es:</label> @Model.Nombre
        <br />
        <label for="apellido">El apellido es:</label> @Model.Apellido
    </div>
    <div class="row">
        <div class="container-fluid">
            <form action="/Home/Index" method="get">
            <h1>Ingreso de Emprendedores</h1>    
                <div class="col-lg-3">
                    <label>Cedula</label>
                    <input type="text" class="form-control" name="Cedula" value="Ingrese su CI" />
                </div>

                <div class="col-lg-3">
                    <label>Nombre</label>
                    <input type="text" class="form-control" name="Nombre" value="Ingrese su Nombre" />
                </div>

                <div class="col-lg-3">
                    <label>Apellido</label>
                    <input type="text" class="form-control" name="Apellido" value="Ingrese su Apellido" />
                </div>

                <div class="col-lg-3">
                    <label>Edad</label>
                    <input type="text" class="form-control" name="Edad" value="Ingrese su Edad" />
                </div>

                <div class="col-lg-3">
                    <button type="submit" class="btn btn-outline succes">Guardar</button>
                </div>
            </form>
          </div>
    </div>
</div>